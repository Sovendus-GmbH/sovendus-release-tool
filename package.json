{
  "name": "sovendus-release-tool",
  "version": "1.4.4",
  "description": "A CLI tool for managing package releases with version bumping and publishing.",
  "license": "GPL-3.0-only",
  "bin": {
    "sovendus-release-tool": "dist/index.mjs"
  },
  "private": false,
  "type": "module",
  "files": [
    "dist/**/*"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./types": {
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "build": "sovendus-builder build",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint --fix",
    "pub": "tsx src/index.ts"
  },
  "dependencies": {
    "commander": "13.1.0",
    "inquirer": "^12.5.2",
    "npm-check-updates": "17.1.18",
    "vite": "^6.3.0"
  },
  "devDependencies": {
    "@types/eslint": "9.6.1",
    "@types/fs-extra": "11.0.4",
    "@types/inquirer": "9.0.7",
    "@types/node": "22.14.1",
    "@typescript-eslint/eslint-plugin": "8.30.1",
    "@typescript-eslint/parser": "8.30.1",
    "eslint": "9.24.0",
    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.2.6",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-react-compiler": "19.0.0-beta-ebf51a3-20250411",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-unused-imports": "4.1.4",
    "fs-extra": "11.3.0",
    "prettier": "3.5.3",
    "prettier-eslint": "16.3.2",
    "prettier-plugin-sort-json": "4.1.1",
    "sovendus-builder": "^1.4.4",
    "tsx": "^4.19.3",
    "typescript": "5.8.3",
    "typescript-eslint": "8.30.1",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "3.1.1"
  }
}